    "author": "Erin Mikail Staples",
    "dependencies": {
      "@langchain/core": "^0.3.0",
      "@langchain/openai": "^0.5.18",
      "@stripe/agent-toolkit": "^0.1.21",
      "axios": "^1.6.0",
      "cors": "^2.8.5",
      "dotenv": "^16.0.0",
      "express": "^4.21.2",
      "langchain": "^0.3.29",
      "openai": "^4.0.0",
      "stripe": "^18.3.0",
      "zod": "^3.22.0"
    },
    "devDependencies": {
      "@types/cors": "^2.8.19",
      "@types/express": "^4.17.23",
      "@types/jest": "^29.0.0",
      "@types/node": "^20.0.0",
      "jest": "^29.0.0",
      "ts-jest": "^29.4.0",
      "ts-node": "^10.9.0",
      "typescript": "^5.0.0"
    }
  }
  