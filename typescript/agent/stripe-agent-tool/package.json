{
    "name": "stripe-agents-sdk-example",
    "version": "1.0.0",
    "description": "A TypeScript Ecommerce agent using Stripe Agent Toolkit with Galileo monitoring",
    "main": "dist/index.js",
    "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "ts-node src/index.ts",
      "interactive": "ts-node src/interactive.ts",
      "customer-service": "ts-node src/interactive.ts",
      "web": "ts-node src/server.ts",
      "web:build": "tsc && node dist/server.js",
      "test": "jest",
      "setup-products": "ts-node scripts/setup_products.ts",
      "debug-prices": "ts-node scripts/debug_prices.ts",
      "test-agent": "ts-node scripts/test_agent_prices.ts"
    },
    "keywords": [
      "stripe",
      "agent",
      "galileo",
      "typescript",
      "ai",
      "ecommerce",
      "evaluations"
