# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_AGENT_ID=your_agent_id

# Galileo (passed to Python service)
GALILEO_PROJECT_NAME=your_project_name
GALILEO_PROTECT_ENABLED=true
GALILEO_PROTECT_STAGE_ID=your_protect_stage_id

# Python Microservice (required - handles all Galileo SDK calls)
# See ../../../python/chatbot/sample-project-voice-chatbot/service/.env.example
GALILEO_PYTHON_SERVICE_URL=http://localhost:8000

# Client-side Config (for UI display)
NEXT_PUBLIC_GALILEO_CONSOLE_URL=https://app.galileo.ai
NEXT_PUBLIC_GALILEO_PROJECT_NAME=your_project_name
NEXT_PUBLIC_GALILEO_PROTECT_ENABLED=true
